<Application
    x:Class="ImageRecognitionSurfAlgorithm.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:conv="clr-namespace:ImageRecognitionSurfAlgorithm.Converters"
    xmlns:local="clr-namespace:ImageRecognitionSurfAlgorithm">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <conv:FilePathToNameConverter x:Key="FilePathToNameConverter" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
